#include<iostream>
using namespace std;
int main(){
	int input;
	
	cout << "Masukkan Bilangan :" ;
	
	cin >>input;
	
	if (input < 0){
		cout << "Bilangan tersebut adalah negatif" << endl;
	} else if (input == 0 ) {
		cout << "Bilangan tersebut adalah netral" << endl;
	} else {
		cout << "Bilangan tersebut adalah positif" << endl;
	} endl;
	
	cout << "Terima Kasih telah Menggunakan Program Tersebut" << endl;
	
	return 0;
}
